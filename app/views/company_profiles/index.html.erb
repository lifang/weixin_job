<div class="area">
  <div class="title clearAfter">
    <h1>微网页</h1>
    <div class="seek">
      <div class="inquire">
        <ul>
          <li>
            <%=link_to new_company_company_profile_path(@company) do%>
              <button>新建微网页</button>
            <%end%>
          </li>
          <!--li style="position: relative;">
            <input type="text" class="search" >
            <button class="searchBtn"></button>
          </li-->
        </ul>
      </div>
    </div>
  </div>
  <div class="content">
    <div class="list clearAfter">
      <%if @company_profiles.blank?%>
        <div style="text-align:center; font-size: 14px;margin-top: 50px;"> 暂无数据
        </div>
      <%else%>
        <ul>
          <%@company_profiles.each do |cp|%>
            <li style="width: 200px;">
              <div class="text">
                <h2><%=cp.title%></h2>
              </div>
              <div class="aFunc" style=" float: right;">
                <%=link_to edit_company_company_profile_path(@company,cp.id),class:"edit_icon tooltip_html" do%>
                    编辑
                  <%end%>
                  <%=link_to  company_company_profile_path(@company,cp.id),method: :delete,confirm:"确定删除？", class:"delete_icon tooltip_html" do%>
                      删除
                    <%end%>
                    <a href="<%=cp.file_path%>" target="_blank" class="show_icon">
                    </a>
                  </div>
                </li>
              <%end%>
            </ul>
          <%end%>
        </div>
      </div>
      <div class="pageTurn">

      </div>
    </div>

    